 .mddtp-picker-date .mddtp-picker__left {
  background-image: url("<%= image_path('ic_keyboard_arrow_left_white_24px.svg') %>") !important;
  left: 4px; }

.mddtp-picker-date .mddtp-picker__right {
  background-image: url("<%= image_path('ic_keyboard_arrow_right_white_24px.svg') %>") !important ;
  right: 4px; }

.mddtp-picker-date .mddtp-picker__left:disabled {
  background-image: url("<%= image_path('ic_keyboard_arrow_left_white_disabled_24px.svg') %>") !important; 
}

.mddtp-picker-date .mddtp-picker__right:disabled {
  background-image: url("<%= image_path('ic_keyboard_arrow_right_white_disabled_24px.svg') %>") !important; 
}
